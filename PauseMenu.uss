.unity-button {
    flex-direction: column;
    align-items: center;
    justify-content: flex-end;
    flex-grow: 0.5;
    font-size: 50px;
    margin-left: 20px;
    margin-right: 20px;
    margin-top: 20px;
    margin-bottom: 20px;
    background-color: rgb(109, 109, 109);
}

.unity-button:hover {
    scale: 1.01 1.1;
    background-color: rgb(170, 169, 169);
}
